/* 近期拜访 */
.recent_visit{
    width: 100%;
    margin: 0 auto;
    background-color: #fff;
    margin-top: 25rpx;
    box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(180, 170, 170, 0.904);
}
.recent_visit .recent_title {
    height: 70rpx;
    line-height: 70rpx;
    color: rgba(51,51,51,.8);
    font-weight: bold;
    font-size: 35rpx;
    padding: 0 10rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
/* 暂无数据 */
.no_data {
    text-align: center;
    color: #B5B5B5;
    font-size: 26rpx;
    padding: 10rpx 0;
}

/* 拜访项目 */
.recent_item {
    margin-top: 25rpx;
    border-bottom: 1rpx solid #eee;
    padding: 5rpx 20rpx;
    box-sizing: border-box;
}
.recent_item .item_top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 65rpx;
    line-height: 65rpx;
}
.recent_item .item_top .left_wrap {
    display: flex;
}
.recent_item .item_top .name {
    color: #B5B5B5;
    font-size: 26rpx;
}
.recent_item .item_top .danwei {
    color: #B5B5B5;
    font-size: 26rpx;
    margin-top: 5rpx;
    margin-left: 15rpx;
}
.recent_item .item_top .time {
    color: #FF3535;
    font-size: 28rpx;
    font-weight: 500;
    /* margin-left: 160rpx; */
}
.recent_item .item_bot {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 65rpx;
    line-height: 65rpx;
}
.recent_item .item_bot .left_wrap {
    display: flex;
}
.recent_item .item_bot .connect {
    color: #B5B5B5;
    font-size: 26rpx;
}
.recent_item .item_bot .phone {
    color: #B5B5B5;
    font-size: 26rpx;
    margin-left: 30rpx;
}
.recent_item .item_bot .xs {
    color: 	#B5B5B5;
    font-size: 26rpx;
    text-align: right;
}

/* 项目 */
.recent_project {
    width: 100%;
    margin: 0 auto;
    background-color: #fff;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(180, 170, 170, 0.904);
}
/* 项目头部 */
.project_head {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 20rpx 0 20rpx;
    box-sizing: border-box;
}
.project_head .head_wrap{
    display: flex;
    align-items: center;
}
.project_head .head_wrap .project_mid{
    margin-left: 30rpx;
}
.project_head .head_wrap .project_mid .name{
    font-size: 28rpx;
    color: rgba(51, 51, 51, .8);
}
.project_head .head_wrap .project_mid .home{
    font-size: 24rpx;
    color: #9C9C9C;
}

/* 项目主体 */
.project_main {
    width: 100%;
    padding: 0rpx 20rpx 10rpx 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #E8E8E8;
}
.show_project_detail {
    display: flex;
    margin: 5rpx 0;
}
.show_project_detail .front_show{
    width: 50%;
    display: flex;
}
.front_show .pro_first{
    color: rgba(51, 51, 51, .8);
    font-size: 26rpx;
    width: 130rpx;
}
.front_show .pro_second{
    color: #9C9C9C;
    font-size: 26rpx;
    flex: 1;
}
.show_project_detail .back_show{
    width: 50%;
    display: flex;
}
.back_show .pro_first{
    width: 130rpx;
    color: rgba(51, 51, 51, .8);
    font-size: 26rpx;
}
.back_show .pro_second{
    color: #9C9C9C;
    font-size: 26rpx;
    flex: 1;
}
/* 项目底部 */
.project_footer {
    width: 100%;
    padding: 0rpx 20rpx 0rpx 20rpx;
    box-sizing: border-box;
}
.project_footer .constract_show {
    padding: 10rpx 0;
    box-sizing: border-box;
}
.constract_show {
    display: flex;
    flex-direction: column;
}
.constract_show .con_name,
.constract_show .con_price
{
    display: flex;
}
.con_con,.pri_pri {
    color: #000;
}
.con_content,.pri_content {
    margin-left: 10rpx;
}
/* 每一行清空padding */
.van-cell {
    padding: 0 !important;
  }
