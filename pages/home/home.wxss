.container {
    background-color: #F5F5F5;
    position: relative;
}
.bg_change_item {
    background-color: #263238 !important;
    /* border: 1px solid #FF3535; */
}

.total_money_num {
    color: #D98BFF;
    font-size: 32rpx;
    font-weight: 400;
}
.total_money {
    color: #fff;
    font-size: 28rpx;
}
.noback_money_num {
    color: #8BEAFF;
    font-size: 32rpx;
    font-weight: 400;
}
.noback_money {
    color: #fff;
    font-size: 28rpx;
}
.wait_money_num {
    color: #72CB8F;
    font-size: 32rpx;
    font-weight: 400;
}
.wait_money {
    color: #fff;
    font-size: 28rpx;
}
.my_project_num,.my_contract_num {
    display: flex;
}
.my_project_num .big_count {
  color: #D98BFF;
    font-size: 36rpx;
}
.my_project_num .mini_count {
    font-size: 26rpx;
    color: rgb(236, 19, 19);
}
.my_contract_num .big_count {
  color: #8BEAFF;
    font-size: 36rpx;
}
.my_contract_num .mini_count {
    font-size: 26rpx;
    color: #FF3535;
}
.my_project,.my_contract {
    color: #fff;
    font-size: 28rpx;
}

/* 近期拜访 */
.recent_visit{
    width: 100%;
    margin: 0 auto;
    background-color: #fff;
    margin-top: 25rpx;
    /* overflow: hidden; */
    box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(180, 170, 170, 0.904);
}
.recent_visit .recent_title {
    height: 70rpx;
    line-height: 70rpx;
    color: #000;
    font-weight: bold;
    font-size: 35rpx;
    padding: 0 10rpx;
    box-sizing: border-box;
    /* border-bottom: 1rpx solid #CFCFCF; */
}
/* 拜访项目 */
.recent_item {
    margin-top: 25rpx;
    border-bottom: 1rpx solid rgba(207, 207 ,207,1);
    padding: 5rpx 20rpx;
    box-sizing: border-box;
    border: 1rpx solid red;
}
.recent_item .item_top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 65rpx;
    line-height: 65rpx;
}
.recent_item .item_top .left_wrap {
    display: flex;
}
.recent_item .item_top .name {
    color: #000;
    font-size: 30rpx;
}
.recent_item .item_top .danwei {
    color: #B5B5B5;
    font-size: 24rpx;
    margin-top: 5rpx;
    margin-left: 15rpx;
}
.recent_item .item_top .time {
    color: #FF3535;
    font-size: 28rpx;
    font-weight: 500;
    /* margin-left: 160rpx; */
}
.recent_item .item_bot {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 65rpx;
    line-height: 65rpx;
}
.recent_item .item_bot .left_wrap {
    display: flex;
}
.recent_item .item_bot .connect {
    color: #B5B5B5;
    font-size: 26rpx;
}
.recent_item .item_bot .phone {
    color: #000;
    font-size: 30rpx;
    margin-left: 30rpx;
}
.recent_item .item_bot .xs {
    color: 	#B5B5B5;
    font-size: 26rpx;
    margin-left: 160rpx;
}

/* 项目 */
.recent_project {
    width: 100%;
    margin: 0 auto;
    background-color: #fff;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(180, 170, 170, 0.904);
}
/* 项目头部 */
.project_head {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 20rpx 0 20rpx;
    box-sizing: border-box;
}
.project_head .head_wrap{
    display: flex;
    align-items: center;
}
.project_head .head_wrap .project_mid{
    margin-left: 30rpx;
}
.project_head .head_wrap .project_mid .name{
    font-size: 28rpx;
}
.project_head .head_wrap .project_mid .home{
    font-size: 24rpx;
    color: #9C9C9C;
}

/* 项目主体 */
.project_main {
    width: 100%;
    border-bottom: 1rpx solid #E8E8E8;
}
/* 状态 */
.project_main .pro_status{
    display: flex;
    height: 50rpx;
    width: 100%;
    align-items: center;
    line-height: 100%;
    padding: 0 25rpx;
    box-sizing: border-box;
}
.project_main .pro_status text{
    line-height: 0;
}
.project_main .pro_status .front{
    color: #000;
    font-size: 26rpx;
}
.project_main .pro_status .back{
    color: #9C9C9C;
    font-size: 26rpx;
    margin-left: 25rpx;
}
/* 收款 */
.project_main .pro_money {
    display: flex;
    justify-content: space-between;
    height: 50rpx;
    width: 100%;
    padding: 0 25rpx;
    box-sizing: border-box;
}
.project_main .pro_money .show{
    display: flex;
    height: 50rpx;
    width: 100%;
    align-items: center;
    line-height: 100%;
}
.project_main .pro_money .show text{
    line-height: 0;
}
.project_main .pro_money .front{
    color: #000;
    font-size: 26rpx;
}
.project_main .pro_money .back{
    color: #9C9C9C;
    font-size: 26rpx;
    margin-left: 25rpx;
}
/* 待收 */
.project_main .pro_money .daishou{
    display: flex;
    height: 50rpx;
    width: 100%;
    align-items: center;
    line-height: 100%;
}
.project_main .pro_money .daishou text{
    line-height: 0;
}

/* 下一阶段 */
.project_main .pro_next{
    display: flex;
    height: 50rpx;
    width: 100%;
    align-items: center;
    line-height: 100%;
    padding: 0 25rpx;
    box-sizing: border-box;
}
.project_main .pro_next text{
    line-height: 0;
}
.project_main .pro_next .front{
    color: #000;
    font-size: 26rpx;
}
.project_main .pro_next .back{
    color: #9C9C9C;
    font-size: 26rpx;
    margin-left: 20rpx;
}
/* 合同 */
.project_main .pro_contract .first{
    display: flex;
    height: 50rpx;
    width: 100%;
    align-items: center;
    line-height: 100%;
    padding: 0 25rpx;
    box-sizing: border-box;
}
.project_main .pro_contract text{
    line-height: 0;
}
.project_main .pro_contract .front{
    color: #000;
    font-size: 26rpx;
}
.project_main .pro_contract .front.not_show{
    opacity: 0;
}
.project_main .pro_contract .back{
    color: #9C9C9C;
    font-size: 26rpx;
    margin-left: 20rpx;
}
/* 回到顶部 */
.back_top {
    position: fixed;
    right: 20rpx;
    bottom: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
/* 底部 */
.footer {
    text-align: center;
    background-color: rgba(100, 20, 145, .5);
}





