.container {
}
.base_info,
.parse_info
{
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
    margin: 30rpx 0;
    padding-left: 31rpx;
}
.parse_info {
    display: flex;
    justify-content: space-between;
}
.show_parse_info {
    flex-shrink: 0;
}
.add_parse_info {
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
}
.wrapper {
    background-color: #f5f5f5;
}
.submit_btn {
    padding: 0 30rpx;
    box-sizing: border-box;
}

/* 阶段表的样式根 */
.cell_root_class {
    margin-bottom: 10rpx;
    border-top: 1px solid #ddd;
}
/* 删除阶段按钮 */
.delete_now_parse {
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}
